<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<BorderPane prefHeight="-1.0" prefWidth="-1.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sevebadajoz.stroketiming.view.AddPracticeScene">
  <bottom>
    <Button mnemonicParsing="false" text="+" BorderPane.alignment="CENTER_RIGHT" />
  </bottom>
  <left>
    <GridPane prefWidth="347.0">
      <children>
        <HBox alignment="CENTER" prefHeight="55.0" prefWidth="371.0" GridPane.columnIndex="0" GridPane.halignment="CENTER" GridPane.hgrow="SOMETIMES" GridPane.rowIndex="0" GridPane.valignment="CENTER" GridPane.vgrow="SOMETIMES">
          <children>
            <TextField fx:id="mFileLocation" focusTraversable="true" prefWidth="200.0" />
            <Button fx:id="mChooseFile" mnemonicParsing="false" onAction="#chooseFile" text="Choose a File (.csv)" />
          </children>
        </HBox>
        <ListView prefHeight="191.0" prefWidth="347.0" GridPane.columnIndex="0" GridPane.rowIndex="2" />
        <Label text="Detected Pieces" GridPane.columnIndex="0" GridPane.halignment="CENTER" GridPane.hgrow="NEVER" GridPane.rowIndex="1" GridPane.vgrow="NEVER" />
      </children>
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      </columnConstraints>
      <rowConstraints>
        <RowConstraints maxHeight="-1.0" minHeight="-1.0" prefHeight="-1.0" vgrow="NEVER" />
        <RowConstraints maxHeight="-1.0" minHeight="10.0" prefHeight="-1.0" vgrow="NEVER" />
        <RowConstraints maxHeight="-1.0" minHeight="-1.0" prefHeight="-1.0" valignment="BOTTOM" vgrow="ALWAYS" />
      </rowConstraints>
    </GridPane>
  </left>
  <top>
    <Button fx:id="mBackButton" mnemonicParsing="false" onAction="#exit" onMouseClicked="#exit" text="&lt;-" />
  </top>
</BorderPane>
